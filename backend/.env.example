# Server Configuration
PORT=3001
NODE_ENV=development

# Database - URL de Conexão PostgreSQL
# Formato: postgresql://usuario:senha@host:porta/nome_banco?schema=public
DATABASE_URL="postgresql://urbanexpansao:urban2026@dashboard_de_expansao_db-expansao:5432/dashboard_de_Expansao?schema=public"

# N8N Database - Banco de dados com dados reais de corridas
# Deixe vazio se não tiver acesso ao banco N8N (usará dados mockados)
N8N_DATABASE_URL="postgresql://n8n_user:n8n_pw@n8n_postgres:5432/postgres?sslmode=disable"

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# IBGE API
IBGE_API_BASE_URL=https://servicodados.ibge.gov.br/api/v3

# Logging
LOG_LEVEL=info
